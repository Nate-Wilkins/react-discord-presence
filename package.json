{
  "name": "react-discord-presence",
  "version": "0.6.0",
  "author": "nate-wilkins <nate-wilkins@code-null.com>",
  "description": "Display your discord presence in react.",
  "main": "dist/index.js",
  "repository": "git@github.com:nate-wilkins/react-discord-presence.git",
  "scripts": {
    "build:css": "cp -r ./src/style/ ./dist/",
    "build:ttf": "cp -r ./src/font/ ./dist/",
    "build:ts": "ts-node ./node_modules/webpack/bin/webpack",
    "build": "rm -rf ./dist && yarn run build:ts && yarn run build:ttf && yarn run build:css",
    "test:lint": "yarn eslint --ext .jsx,.js,.ts,.tsx ./",
    "test": "yarn jest --coverage && yarn test:lint"
  },
  "dependencies": {
    "color": "4.2.3",
    "react-twemoji": "0.5.0",
    "schema-lanyard-discord-presence": "0.6.0"
  },
  "peerDependencies": {
    "react": ">=15.0.1",
    "react-dom": ">=15.0.1"
  },
  "devDependencies": {
    "react": ">=15.0.1",
    "react-dom": ">=15.0.1",
    "@testing-library/react": "13.4.0",
    "@types/circular-dependency-plugin": "5.0.2",
    "@types/color": "3.0.3",
    "@types/jest": "29.4.0",
    "@types/react": "18.0.26",
    "@types/react-dom": "18.0.8",
    "@types/react-twemoji": "0.4.0",
    "@typescript-eslint/eslint-plugin": "5.49.0",
    "@typescript-eslint/parser": "5.49.0",
    "circular-dependency-plugin": "5.2.2",
    "eslint": "7.17.0",
    "eslint-plugin-bdd": "2.1.1",
    "eslint-plugin-jest": "26.0.0",
    "eslint-plugin-jsort": "1.1.2",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-react": "7.29.4",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-testing-library": "5.10.0",
    "jest": "29.4.1",
    "jest-date-mock": "1.0.8",
    "jest-environment-jsdom": "29.3.1",
    "path-browserify": "1.0.1",
    "prettier": "1.19.1",
    "source-map-loader": "2.0.1",
    "ts-jest": "29.0.5",
    "ts-loader": "9.1.1",
    "ts-node": "10.9.1",
    "typescript": "4.6.4",
    "webpack": "5.36.2",
    "webpack-cli": "4.6.0"
  }
}
