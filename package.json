{
  "name": "react-discord-presence",
  "version": "6.0.2",
  "author": "nate-wilkins <nate-wilkins@code-null.com>",
  "description": "Display your Discord presence in react.",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "http://github.com/nate-wilkins/react-discord-presence"
  },
  "homepage": "http://github.com/nate-wilkins/react-discord-presence",
  "bugs": "http://github.com/nate-wilkins/react-discord-presence/issues",
  "scripts": {
    "build:css": "mkdir -p ./dist/src/display/ && cp -r ./src/display/style/ ./dist/src/display/",
    "build:ttf": "mkdir -p ./dist/src/display/ && cp -r ./src/display/font/ ./dist/src/display/",
    "build:image": "mkdir -p ./dist/deps/discord-badges/ && cp -r ./deps/discord-badges/assets/ ./dist/deps/discord-badges/",
    "build:ts": "ts-node ./node_modules/webpack/bin/webpack",
    "build": "rm -rf ./dist && yarn run build:ts && yarn run build:ttf && yarn run build:image && yarn run build:css",
    "test:lint": "yarn run eslint --ext .jsx,.js,.ts,.tsx ./",
    "test": "yarn run jest --coverage && yarn run test:lint",
    "storybook": "start-storybook -p $STORYBOOK_PORT",
    "storybook:build": "yarn run build-storybook -o ./storybook"
  },
  "dependencies": {
    "color": "4.2.3",
    "data-accessor": "11.0.0",
    "date-fns": "2.29.3",
    "react-twemoji": "0.5.0",
    "schema-lanyard-discord-presence": "0.6.0",
    "use-error-boundary": "2.0.6"
  },
  "peerDependencies": {
    "react": ">=15.0.1",
    "react-dom": ">=15.0.1"
  },
  "devDependencies": {
    "@storybook/addon-actions": "6.5.16",
    "@storybook/addon-console": "1.2.3",
    "@storybook/addon-docs": "6.5.16",
    "@storybook/addon-essentials": "6.5.16",
    "@storybook/addon-links": "6.5.16",
    "@storybook/addon-postcss": "2.0.0",
    "@storybook/addon-storysource": "6.5.16",
    "@storybook/react": "6.5.16",
    "@testing-library/react": "13.4.0",
    "@types/circular-dependency-plugin": "5.0.2",
    "@types/color": "3.0.3",
    "@types/jest": "29.4.0",
    "@types/react": "18.0.26",
    "@types/react-dom": "18.0.8",
    "@types/react-twemoji": "0.4.0",
    "@typescript-eslint/eslint-plugin": "5.49.0",
    "@typescript-eslint/parser": "5.49.0",
    "circular-dependency-plugin": "5.2.2",
    "eslint": "7.17.0",
    "eslint-plugin-bdd": "2.1.1",
    "eslint-plugin-jest": "26.0.0",
    "eslint-plugin-jsort": "1.1.2",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-react": "7.29.4",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-testing-library": "5.10.0",
    "fetch-mock": "9.11.0",
    "jest": "29.4.1",
    "jest-date-mock": "1.0.8",
    "jest-environment-jsdom": "29.3.1",
    "path-browserify": "1.0.1",
    "prettier": "1.19.1",
    "puppeteer": "19.6.3",
    "react": ">=15.0.1",
    "react-dom": ">=15.0.1",
    "source-map-loader": "2.0.1",
    "storybook-css-modules": "1.0.8",
    "storycap": "4.0.0",
    "ts-jest": "29.0.5",
    "ts-loader": "9.1.1",
    "ts-node": "10.9.1",
    "typescript": "4.6.4",
    "webpack": "5.36.2",
    "webpack-cli": "4.6.0"
  }
}
